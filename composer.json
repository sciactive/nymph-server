{
  "name": "sciactive/nymph-server",
  "description": "Powerful object data storage and querying for collaborative web apps.",
  "version": "3.2.2",
  "time": "2020-08-14",
  "homepage": "http://nymph.io/",
  "type": "library",
  "scripts": {
    "lint": "phpcs -vs --extensions=php --ignore=vendor/* --runtime-set ignore_warnings_on_exit 1 .",
    "format": "phpcbf -vs --extensions=php --ignore=vendor/* --runtime-set ignore_warnings_on_exit 1 ."
  },
  "authors": [
    {
      "name": "Hunter Perrin",
      "email": "hunter@sciactive.com",
      "homepage": "https://github.com/hperrin",
      "role": "Developer"
    }
  ],
  "keywords": [
    "orm",
    "mysql",
    "postgres",
    "postgresql",
    "object database",
    "object data store",
    "object relational mapper",
    "object relational mapping"
  ],
  "license": "Apache-2.0",
  "support": {
    "issues": "https://github.com/sciactive/nymph/issues",
    "wiki": "https://github.com/sciactive/nymph/wiki",
    "source": "https://github.com/sciactive/nymph-server"
  },
  "archive": {
    "exclude": [
      "**/.*",
      ".travis.yml",
      "testing",
      "vendor"
    ]
  },
  "autoload": {
    "psr-0": {
      "Nymph\\": "src/"
    },
    "psr-4": {
      "Nymph\\": "src/"
    }
  },
  "require-dev": {
    "sciactive/hookphp": "~2.1.0",
    "squizlabs/php_codesniffer": "3.*",
    "phpunit/phpunit": "^7.1"
  }
}
